local function ah_spawn(name)
	local tbl = aditional_spawn_table[name]
	for k, v in pairs(tbl) do
		for i = 1, (v[6] or 1) do 
			alife():create(k, vector():set(v[1], v[2], v[3]), v[4], v[5])
		end
	end
end

